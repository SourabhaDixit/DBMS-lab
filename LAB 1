**Schema (MySQL v5.7)**

    create table BOOK(Book_id integer primary key,Title varchar(50),Publisher_Name varchar(50),pub_Year varchar(30));
    insert into BOOK values(01,'DBMS','Sumitha','1998');
    insert into BOOK values(02,'ME','Shakuntala','1989');
    insert into BOOK values(03,'SE','Venkatesh','1956'); 
    insert into BOOK values(04,'ATC','Sanketh','1879'); 
    insert into BOOK values(05,'ADP','Sumath','1854');
    insert into BOOK values(06,'DBMS','shakthi','1789');   
    
    create table BOOK_COPIES(Book_id integer,Branch_id integer,No_of_Copies integer, primary key(Book_id,Branch_id));                                                                                                            

